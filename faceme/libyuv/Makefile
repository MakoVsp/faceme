dist: 
	@test -d tmp/yuv1.0.0 || mkdir -p tmp/yuv1.0.0
	$(COPY_FILE) --parents $(DIST) tmp/yuv1.0.0/ && (cd `dirname tmp/yuv1.0.0` && $(TAR) yuv1.0.0.tar yuv1.0.0 && $(COMPRESS) yuv1.0.0.tar) && $(MOVE) `dirname tmp/yuv1.0.0`/yuv1.0.0.tar.gz . && $(DEL_FILE) -r tmp/yuv1.0.0


