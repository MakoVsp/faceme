####### Install

install_target: first FORCE
	@test -d $(INSTALL_ROOT)/data/apps/com.syberos.faceme/libs/ || mkdir -p $(INSTALL_ROOT)/data/apps/com.syberos.faceme/libs/
	-$(INSTALL_PROGRAM) "$(TARGET)" "$(INSTALL_ROOT)/data/apps/com.syberos.faceme/libs/$(TARGET)"
	-$(STRIP) --strip-unneeded "$(INSTALL_ROOT)/data/apps/com.syberos.faceme/libs/$(TARGET)"
	-$(SYMLINK) "$(TARGET)" "$(INSTALL_ROOT)/data/apps/com.syberos.faceme/libs/$(TARGET0)"
	-$(SYMLINK) "$(TARGET)" "$(INSTALL_ROOT)/data/apps/com.syberos.faceme/libs/$(TARGET1)"
	-$(SYMLINK) "$(TARGET)" "$(INSTALL_ROOT)/data/apps/com.syberos.faceme/libs/$(TARGET2)"

uninstall_target: FORCE
	-$(DEL_FILE) "$(INSTALL_ROOT)/data/apps/com.syberos.faceme/libs/$(TARGET)" 
	 -$(DEL_FILE) "$(INSTALL_ROOT)/data/apps/com.syberos.faceme/libs/$(TARGET0)" 
	 -$(DEL_FILE) "$(INSTALL_ROOT)/data/apps/com.syberos.faceme/libs/$(TARGET1)" 
	 -$(DEL_FILE) "$(INSTALL_ROOT)/data/apps/com.syberos.faceme/libs/$(TARGET2)"
	-$(DEL_DIR) $(INSTALL_ROOT)/data/apps/com.syberos.faceme/libs/ 


install:  install_target  FORCE

uninstall: uninstall_target   FORCE

FORCE:

